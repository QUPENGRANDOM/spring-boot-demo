<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pengq.springboot.mybatisplus.dao.UserDao">
    <select id="findAllBySex" parameterType="java.lang.Integer" resultType="pengq.springboot.mybatisplus.entity.Users">
        SELECT * FROM `users` WHERE `sex` = #{sex};
    </select>
</mapper>